mber);\n    }\n\n    useEffect(() => { \n       if(!allDogs.length > 0) {dispatch(getDogs())\n        dispatch(getTemperaments())}\n        dispatch(clearDetail())\n    }, [dispatch])\n\n    function handleClick(e) {\n        e.preventDefault();\n        dispatch(getDogs())\n        setCurrentPage(1);\n    }\n\n    function handleFilterTemperaments(e) {\n        e.preventDefault(e);\n        dispatch(filterDogsByTemperament(e.target.value))\n        setClean({\n            ...clean, temperaments:e.target.value\n        }); \n        e.target.value = \"Filter Temperaments\"\n        setCurrentPage(1);\n        setOrden(e.target.value);\n        \n    }\n    function handleFilterCreated(e) {\n        e.preventDefault();\n        dispatch(filterCreated(e.target.value))\n        setClean({\n            ...clean, created:e.target.value\n        }); \n        e.target.value = \"Filter by Origin\"\n        setCurrentPage(1);\n        setOrden(e.target.value);\n    }\n\n    function handleSortByName(e) {\n        e.preventDefault();\n        dispatch(sortByName(e.target.value));\n        setClean({\n            ...clean, name:e.target.value\n        }); \n        e.target.value = \"Sort by name\"\n        setCurrentPage(1);\n        setOrden(e.target.value);\n    }\n    function handleSortByWeight(e) {\n        e.preventDefault();\n        dispatch(sortByWeight(e.target.value));\n        setClean({\n            ...clean, weight:e.target.value\n        }); \n        e.target.value=\"Sort by weight\"\n        setCurrentPage(1);\n        setOrden(`Ordenado ${e.target.value}`);\n    }\n    \n    return (\n        <div>\n            {currentDogs.length > 0 ?\n            <div className=\"home\">\n                <div className=\"papeiners\">\n                    <hr></hr>\n                </div>   \n                <div className=\"header\">\n                    <img src={dog} alt=\"img not found\" />\n                    <p><strong>Dogs</strong></p>\n                    <h3>Unconditional Love</h3>\n                    <div className=\"padr\">\n                        <Link to='/dogs'>\n                            <button className='elementNA'> \n                                Create Dog\n                            </button>\n                        </Link>\n                    </div>\n                    <div className=\"padl\">\n                        <button className='elementNA' onClick={e => { handleClick(e) }} >\n                           All Dogs\n                        </button>\n                    </div>\n                    <div className=\"container\">\n                        <div className=\"pad\">\n                            <SearchBar \n                            setCurrentPage={setCurrentPage} />\n                        </div>\n                    </div>\n                    <div className=\"padw\">\n                        <select  className='elementNC'defaultValue='selected' onChange={e => handleSortByName(e)} >\n                            <option hidden value=\"Sort by name\">{clean.name || \"Sort by name\"}</option>\n                            <option value='selected' disabled selected>Breed</option>\n                            <option value='Ascendent'>A - Z</option>\n                            <option value='Descendent'>Z - A</option>\n                        </select>\n                    </div>\n                    <div className=\"padq\">\n                        <select className=\"elementNC\" defaultValue='selected' onChange={e => handleSortByWeight(e)}  >\n                            <option hidden value=\"Sort by weight\">{clean.weight || \"Sort by weight\"}</option>\n                            <option value='selected' disabled selected>Weight</option>\n                            <option value='Lighter to heavier'>Lighter to heavier</option>\n                            <option value='Heavier to lighter'>Heavier to lighter</option>\n                        </select>\n                    </div>\n                    <div className=\"pade\">\n                        <select className='elementNC' defaultValue='selected' onChange={e => handleFilterCreated(e)}>\n                            <option hidden value=\"Filter by Origin\">{clean.created || \"Filter by Origin\"}</option>\n                            <option value=\"selected\" disabled selected >Origin</option>\n                            <option value=\"Api Dogs\">API</option>\n                            <option value=\"In Database\">DB</option>\n                        </select>\n                    </div>\n                    <div className=\"padf\">\n                        <select className='elementNC' defaultValue='selected' name='temperaments' onChange={e => handleFilterTemperaments(e)} >\n                            <option hidden value=\"Filter Temperaments\">\n                                {clean.temperaments || \"Filter Temperaments\"}\n                            </option>\n                            <option value='selected' disabled selected>Temperaments</option>\n                            <option value='all'>All</option>\n                            {allTemperaments.map(t => (\n                            <option key={t.id} value={t.name}>{t.name}</option>\n                            ))}\n                        </select>\n                    </div>\n                </div>\n                <div className='cardsContainer'>\n                    {currentDogs?.map(d => {\n                    return (\n                        <Card \n                        key={d.id}\n                        id={d.id}\n                        image={d.image}\n                        name={d.name}\n                        temperaments={d.temperaments}\n                        weightMin={d.weightMin}\n                        weightMax={d.weightMax}\n                        heightMin={d.heightMin}\n                        heightMax={d.heightMax}\n                         />\n                    )})}\n                </div>\n            <div >\n            {<Paginado \n                dogsPerPage={dogsPerPage}\n                allDogs={allDogs.length}\n                paginado = {paginado}\n                currentPage={currentPage}/>}\n            </div>\n        </div>: <Loading/>} \n        </div>\n    )\n}\n","/home/pato/Escritorio/henry2/Dogs/client/src/components/Form.jsx",[],"/home/pato/Escritorio/henry2/Dogs/client/src/components/Detail.jsx",["69"],"import React from \"react\";\nimport { useHistory } from 'react-router-dom';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { getDetail } from '../redux/actions/index';\nimport { useEffect } from \"react\";\nimport '../styles/Detail.css';\nimport Loading from \"./Loading\";\n\nexport default function Detail(props){\n    const dispatch = useDispatch();\n    const history=useHistory();\n    const myDog = useSelector((state) => state.detail)\n\n    useEffect(() => {\n        dispatch(getDetail(props.match.params.id));\n        \n    },[dispatch])\n\n    function handleHistory(){\n        history.push(\"/home\")\n    }\n\n    return (\n        <div className=\"detail-container\">\n            {\n                myDog.length?\n                <div className='detail-card'>\n                    <div top-row background-top-row>\n                    <h1 className=\"top-row background-top-row\"> {myDog[0].name}</h1>\n                    </div>\n                    <img src={myDog[0].image} alt={myDog[0].name} ></img>\n                    <h2 className=\"temp\">{myDog[0].createdInDb === true ? myDog[0].temperaments?.map(t => t.name +\"/ \") :  myDog[0].temperaments?.join(', ')}</h2>\n                    <br/>\n                    <h3>Weight:     {myDog[0].weightMin} - {myDog[0].weightMax}</h3>\n                    <h3>Height:     {myDog[0].heightMin} - {myDog[0].heightMax}</h3>\n                    <h3>Lifespan:     {myDog[0].lifespan}</h3>\n                    <button onClick={handleHistory} className=\"back\"><strong><h1>Back</h1></strong></button>\n                </div>\n                : <Loading/>\n            }\n        </div>\n    )\n}\n","/home/pato/Escritorio/henry2/Dogs/client/src/redux/actions/index.js",[],"/home/pato/Escritorio/henry2/Dogs/client/src/components/Card.jsx",[],"/home/pato/Escritorio/henry2/Dogs/client/src/components/SearchBar.jsx",["70"],"import React, {useState, useEffect} from 'react';\nimport { useDispatch } from 'react-redux';\nimport { getDogs } from '../redux/actions';\nimport '../styles/SearchBar.css';\nimport { FaSearch } from 'react-icons/fa';\n\nfunction SearchBar({setCurrentPage}) {\n    const dispatch = useDispatch()\n    const [name, setName] = useState(\"\");\n\n    function handleInputChange(e){\n        e.preventDefault()\n        setName(e.target.value)\n    }\n     function handleSubmit(e){\n        e.preventDefault();\n        dispatch(getDogs(name));\n        setCurrentPage(1);\n     }\n    return (\n        <> \n       \n            <input\n                className='input'\n                type='search'\n                placeholder=' Search Dog ...'\n                onChange={ handleInputChange }\n                value={name}\n            /> <button className='searcho' type='search' onClick={handleSubmit}><FaSearch/></button>\n            \n        </>\n    )\n}\n\nexport default SearchBar","/home/pato/Escritorio/henry2/Dogs/client/src/components/Paginado.jsx",["71"],"import React from 'react';\nimport styles from '../styles/Paginado.css'\n\nexport default function Paginado({dogsPerPage, currentPage, allDogs, paginado}){\n    const pageNumbers = []\n    for (let i = 1; i <= Math.ceil(allDogs/dogsPerPage); i++) {\n        pageNumbers.push(i)\n    }\n\n    return(\n        <nav>\n          <div className='flechas-container'>\n            {\n              currentPage > 1 ? <button className='flechas' onClick={()=>paginado(currentPage - 1)}> ❮ </button>:\n              <button className='flechas' disabled> ❮ </button>\n            }\n             {\n              currentPage < pageNumbers.length ? <button className='flechas' onClick={()=>paginado(currentPage + 1)}> ❯ </button>:\n              <button className='flechas' disabled> ❯ </button>\n            }\n          </div>\n            <div className='paginado'>\n        {\n          pageNumbers?.map(num=>(\n            <span key={num}>\n              <button className='number' onClick={()=>paginado(num)} ><strong>{num}</strong></button>\n            </span>\n          ))\n        }\n           </div>\n    </nav>\n      )\n    }\n        \n","/home/pato/Escritorio/henry2/Dogs/client/src/components/Loading.jsx",["72"],"import React from 'react';\nimport loading from '../styles/loading-dog.gif';\nimport '../styles/Loading.css';\nimport '../styles/Paginado.css';\nimport { useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\n\nexport default function Loading(){\n    const allDogs = useSelector((state) => state.dogs);\n\n    return (\n        <div>\n        \n         <div >\n                <img className='loading' src={loading} alt='Loading'/>\n                <div className='button-div'><Link to=\"/\"><button className='flechas'> ❮ </button></Link></div>\n            </div>\n        </div>\n\n    )\n} ","/home/pato/Escritorio/henry2/Dogs/client/src/redux/reducer/index.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":26,"column":25,"nodeType":"79","messageId":"80","endLine":26,"endColumn":39},{"ruleId":"77","severity":1,"message":"81","line":30,"column":12,"nodeType":"79","messageId":"80","endLine":30,"endColumn":18},{"ruleId":"82","severity":1,"message":"83","line":40,"column":8,"nodeType":"84","endLine":40,"endColumn":18,"suggestions":"85"},{"ruleId":"82","severity":1,"message":"86","line":17,"column":7,"nodeType":"84","endLine":17,"endColumn":17,"suggestions":"87"},{"ruleId":"77","severity":1,"message":"88","line":1,"column":26,"nodeType":